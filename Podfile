platform :ios, '8.0'
use_frameworks!
# 去掉由pod引入的第三方库的警告，需要更新命令才生效
inhibit_all_warnings!

target 'iOSAPP' do
 #数据请求
 pod 'AFNetworking'
 #刷新
 pod 'MJRefresh'
 #数据处理
 pod 'YYModel'
 #图片加载
 pod 'SDWebImage'
 #数据库
 pod 'FMDB'
 #加载框
 pod 'MBProgressHUD'
 #Toast
 pod 'SHToast'
 #复杂滚动布局
 pod 'SHScrollLayout'
 #拓展
 pod 'SHExtension'
 #滚动视图
 pod 'SHScrollView'
 #Alert弹窗
 pod 'SHAlertView'
 #Sheet弹窗
 pod 'SHActionSheetView'
 #音视频播放
 pod 'SHAVPlayer'
end
