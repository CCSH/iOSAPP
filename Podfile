platform :ios, '8.0'
use_frameworks!
# 去掉由pod引入的第三方库的警告，需要更新命令才生效
inhibit_all_warnings!

target 'iOSAPP' do
 #数据请求
 pod 'AFNetworking', '~> 3.1.0'
 #刷新
 pod 'MJRefresh', '~> 3.1.12'
 #数据处理
 pod 'MJExtension', '~> 3.0.13'
 #图片加载
 pod 'SDWebImage', '~> 4.1.0'
 #数据库
 pod 'FMDB', '~> 2.7.2'
 #加载框
 pod 'MBProgressHUD', '~> 1.0.0'

end
